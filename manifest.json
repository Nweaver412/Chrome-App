{
  "manifest_version": 3,
  "name": "GitHub Shared Projects",
  "version": "1.0",
  "description": "Access GitHub repositories shared with you.",
  "permissions": ["identity", "storage"],
  "host_permissions": ["https://api.github.com/*"],
  "action": {
    "default_popup": "web/popup.html",
    "default_icon": "assets/icon96.png"
  }
}